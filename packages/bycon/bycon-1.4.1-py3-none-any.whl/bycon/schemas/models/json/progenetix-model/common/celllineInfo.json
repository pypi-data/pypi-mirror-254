{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": false,
    "description": "Definition of cellline information",
    "properties": {
        "id": {
            "description": "The cellosaurus identifier or another id as CURIE.",
            "example": "cellosaurus:CVCL_0004",
            "type": "string"
        },
        "label": {
            "description": "The common name of the cellline",
            "example": [
                "Namalwa",
                "HL60"
            ],
            "type": "string"
        },
        "notes": {
            "description": "An optional text descriptor.",
            "example": "Derived from HeLa",
            "type": "string"
        },
        "reference": {
            "description": "A url.",
            "example": "https://www.cellosaurus.org/CVCL_IS01",
            "type": "string"
        },
        "synonyms": {
            "description": "other names & identifiers for the cellline",
            "items": "string",
            "type": "array"
        }
    },
    "title": "Cellline Info",
    "type": "object"
}