{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "TestEntity",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "path": {
            "type": "object",
            "properties": {
                "_pickle_": {
                    "type": "boolean",
                    "const": true
                },
                "data": {
                    "type": "string"
                }
            },
            "required": [
                "_pickle_",
                "data"
            ]
        },
        "born_time": {
            "type": "integer"
        },
        "dead_time": {
            "type": "integer"
        },
        "auto_save": {
            "type": "boolean"
        }
    },
    "required": [
        "name",
        "path",
        "born_time",
        "dead_time",
        "auto_save"
    ]
}